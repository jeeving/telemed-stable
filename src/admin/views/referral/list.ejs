<% layout('../layouts/list') -%>
<div class="row marginLess25">
    <div class="col-12">
        <div class="page-title-box">
            <div class="page-title-left">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item">Dashboard</li>
                    <li class="breadcrumb-item active"><a href="javascript: void(0);">Referrals</a></li>
                </ol>
            </div>
        </div>
    </div>
    <div class="col-sm-12">
        <div class="page-title-box">
            <h4 class="page-title">Referrals</h4>
        </div>
    </div>
</div>
<div class="col-lg-12 text-right">
    
  </div>
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <table class="table nowrap" id="calls-datatable">
                    <thead>
                    <tr>
                        <th>S.NO.</th>
                        <th>From</th>
                        <th>To</th>
                        <th>Time</th>
                    </tr>
                    </thead>
                    <tbody>
                        <% referrals.forEach( (referral,i)=>{ %>
                            <tr>
                                <td><%= i+1  %></td>
                                <td><%= referral.senderId.fullName  %></td>
                                <td><%= referral.receiverId.fullName  %></td>
                                <td><%=  moment.unix(referral.msgTime).format("YYYY-MM-DD hh:mm A")   %></td>
                                
                                
                            </tr>
                        <% }) %>

                        


                    </tbody>
                </table>
                <div class="row tableLoader">
                    <div class="col-sm-12 text-center">
                        <!-- <div class="spinner-border avatar-md text-primary m-2" role="status"></div> -->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
