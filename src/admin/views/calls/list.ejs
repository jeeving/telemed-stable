<% layout('../layouts/list') -%>
<div class="row marginLess25">
    <div class="col-12">
        <div class="page-title-box">
            <div class="page-title-left">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item">Dashboard</li>
                    <li class="breadcrumb-item active"><a href="javascript: void(0);">Calls</a></li>
                </ol>
            </div>
        </div>
    </div>
    <div class="col-sm-12">
        <div class="page-title-box">
            <h4 class="page-title">Calls</h4>
        </div>
    </div>
</div>
<div class="col-lg-12 text-right">
    
  </div>
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <table class="table nowrap" id="calls-datatable">
                    <thead>
                    <tr>
                        <th>S.NO.</th>
                        <th>From</th>
                        <th>To</th>
                        <th>Type</th>
                        <th>Duration(Second)</th>
                        <th>Amount</th>
                        <th>Date</th>
                    </tr>
                    </thead>
                    <tbody>
                        <% calls.forEach( (call,i)=>{ %>
                            <tr>
                                <td><%= i+1  %></td>
                                <td><%= call.callerId.fullName  %></td>
                                <td><%= call.receiverId.fullName  %></td>
                                <td><%= call.type  %></td>
                                <td><%= call.CallDuration  %></td>
                                <td>
                                    <% if( !call.organizationId ){ %>
                                       Rs. <%= call.amount  %>
                                    <% }%>
                                    
                                </td>
                                <th><%= moment(call.created).format("YYYY-MM-DD hh:mm A")  %></th>
                                
                            </tr>
                        <% }) %>

                        


                    </tbody>
                </table>
                <div class="row tableLoader">
                    <div class="col-sm-12 text-center">
                        <!-- <div class="spinner-border avatar-md text-primary m-2" role="status"></div> -->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
