<% layout('../layouts/list') -%>
<div class="row marginLess25">
    <div class="col-12">
        <div class="page-title-box">
            <div class="page-title-left">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item">Dashboard</li>
                    <li class="breadcrumb-item active"><a href="javascript: void(0);">Report</a></li>
                </ol>
            </div>
        </div>
    </div>
    <div class="col-sm-12">
        <div class="page-title-box">
            <h4 class="page-title">View User Report</h4>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <div class="row">

                    <div class="col-lg-12">
                        <div class="form-group mb-3">
                            <label>Name: </label> <%= reports[0].name || 'N/A' %>
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="form-group mb-3">
                            <label>Message: </label> <%= reports[0].reseon || 'N/A' %>
                        </div>
                    </div>
 
                    <div class="col-lg-6">
                        <div class="form-group mb-3">
                            <label>Created at: </label> <%= showDateTimeZone(reports[0].timestamp) || 'N/A' %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>